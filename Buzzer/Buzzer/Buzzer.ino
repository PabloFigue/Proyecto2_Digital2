void loop() {
  if (Serial2.available()) {
    //Serial.println("entre a la interrupcion");
    //while(Serial2.available()){
    char data = Serial2.read();
    Serial.println(data);
    opcion = data;
    Serial.println(opcion);
    }
if(bandera ==1){    
  if (opcion == 49){
    sonido5(); //Inicio del juego
  }else if (opcion == 50){
    sonido6();  //menu
  }else if (opcion == 51){
    sonido13(); //Juego
  }else if (opcion == 52){
    sonido12(); //GanaFelix
  }else if (opcion == 53){
    sonido21(); //GanaRalph
  }else if (opcion == 54){
    noTone(tonePin); //Mutear
    bandera = 0;
  }else if (opcion == 55){
    sonido11(); //creditos
  }else{
    delay(10);
    noTone(tonePin);        // detiene reproduccion de tono
  }
}else if(bandera ==0 && opcion == 54){
  bandera =1;
  opcion=50;
}
}



